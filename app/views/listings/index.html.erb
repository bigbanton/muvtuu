<div class="jumbotron">
<h1>Find the best apartment deals around Atlanta.</h1>
<h2>Exclusive marketplace for move in specials and more.</h2>
</div>

<div class="center">
  <div class="row">
    <% @listings.each do |listing| %>
      <div class="col-md-3">
          <div class="thumbnail">
          <%= link_to image_tag(listing.image.url), listing %>

          <div class="caption">
            <h3><%= listing.name %></h3>
            <p><%= listing.price %></p>
            
            
      </div>
    </div>
   </div>
     
<% end %>
</div>
</div>


<br>
  
<% if user_signed_in? %>
  <%= link_to 'New Listing', new_listing_path, class: "btn btn-link" %>
<% end %>
